            android:background="@color/not_used"
            android:gravity="center"
            android:text="407"
            android:textSize="18sp" />

        <TextView
            android:layout_width="40dp"
            android:layout_height="80dp"
            android:background="@color/not_used"
            android:gravity="center"
            android:text="406"
            android:textSize="18sp" />

        <TextView
            android:layout_width="120dp"
            android:layout_height="80dp"
            android:background="@color/not_used"
            android:gravity="center"
            android:text="405"
            android:textSize="18sp" />

    </LinearLayout>


</RelativeLayout>